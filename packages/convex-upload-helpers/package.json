{
  "name": "@battlemagedotapp/convex-upload-helpers",
  "version": "1.0.0",
  "description": "Convex upload helpers",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./styles": {
      "types": "./dist/index.css.map",
      "default": "./dist/index.css"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint .",
    "check-types": "tsc --noEmit",
    "check-deps": "knip"
  },
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "knip": "^5.63.0",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.40.0"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "dependencies": {
    "convex": "^1.26.1",
    "sonner": "^2.0.7"
  }
}
